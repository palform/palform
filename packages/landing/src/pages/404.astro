---
import Html from "../layouts/Html.astro";
import InfoPageTitle from "../components/info/InfoPageTitle.astro";
import ResponsivePadding from "../components/ResponsivePadding.astro";
import InfoPageParagraph from "../components/info/InfoPageParagraph.astro";
import Navbar from "../components/navbar/Navbar.svelte";
---

<Html title="Page not found">
	<Navbar />

	<ResponsivePadding class="mt-32">
		<InfoPageTitle>Page not found :(</InfoPageTitle>
		<InfoPageParagraph>
			If something is broken, please <a
				href="https://docs.palform.app/"
				class="underline">let us know</a
			>.
		</InfoPageParagraph>
	</ResponsivePadding>
</Html>
