{
    "extends": ["astro/tsconfigs/strict", "@tsconfig/svelte/tsconfig.json"],
    "include": [
        "src/**/*.ts",
        "src/**/*.js",
        "src/**/*.svelte",
        "src/**/*.astro"
    ],
    "compilerOptions": {
        "target": "ESNext",
        "plugins": [
            {
                "name": "typescript-svelte-plugin",
                "enabled": true,
                "assumeIsSvelteProject": true
            }
        ]
    }
}
